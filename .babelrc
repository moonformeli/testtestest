{
	"presets": [
		"react",
		[
			"env",
			{
				"targets": {
					"browsers": [
						"last 2 versions"
					]
				}
			}
		],
		"stage-0"
	],
	"plugins": [
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy": true
			}
		],
		[
			"@babel/plugin-proposal-class-properties",
			{
				"loose": true
			}
		]
	],
}